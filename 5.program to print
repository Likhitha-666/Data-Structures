#include<stdio.h>
int main()
{
    int  arr[]={1,2,3,4,5,6,7,8,9},i;
    int size=sizeof(arr)/sizeof(arr[0]);
    int max=arr[0];
    int min=arr[0];
    for(i=1;i<size;i++)
    {
        if(arr[i]>max)
        max=arr[i];
        if (arr[i]<min)
        min=arr[i];
        
    }
    printf("the maximum element is %d\n",max);
    printf("the minimum element is %d",min);
    return 0;
}
